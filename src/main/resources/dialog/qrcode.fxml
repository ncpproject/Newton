<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" styleClass="dialog" stylesheets="@../mainView/mainView.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <VBox fx:id="dialogVbox" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="qrimage" cache="true" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="50.0" left="75.0" right="50.0" top="50.0" />
               </VBox.margin>
            </ImageView>
            <Label fx:id="addressLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="18.0" prefWidth="451.0" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
               <effect>
                  <Blend opacity="0.14">
                     <topInput>
                        <Shadow />
                     </topInput>
                  </Blend>
               </effect>
               <font>
                  <Font name="Century" size="15.0" />
               </font></Label>
            <Button fx:id="closeButton" mnemonicParsing="false" text="Close">
               <VBox.margin>
                  <Insets bottom="50.0" left="200.0" />
               </VBox.margin>
            </Button>

         </children>
       </VBox>
   </children>
</AnchorPane>
